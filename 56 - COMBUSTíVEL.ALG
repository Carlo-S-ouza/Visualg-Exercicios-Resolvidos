Algoritmo "Combustível "

Var
   litros, kmL, total: real

Inicio
   Escreval("Quantos litros você abasteceu?")
   leia(litros)
   Escreval("Quantos KM seu carro percorre por litro?")
   leia(kmL)
   limpatela

   total <- (litros*kmL)
   Escreval("Você poderá percorrer até ", total, "km")

Fimalgoritmo