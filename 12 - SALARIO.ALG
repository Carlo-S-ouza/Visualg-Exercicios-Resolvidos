Algoritmo "12 - Salario"

Var
   num1, num2, resultado: real

Inicio
   escreval("Quanto o funcionário ganha por hora?")
   leia (num1)
   escreval("Número de horas trabalhadas:")
   leia (num2)

   resultado <- num1*num2
   limpatela
   escreval("O funcionário deve receber R$", resultado)

Fimalgoritmo
