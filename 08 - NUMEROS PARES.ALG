Algoritmo "8 - Numeros Pares"

Var
   valor, cont: inteiro

Inicio

   escreval("Digite um número inteiro.")
   leia (valor)
   limpatela

   enquanto (cont <= valor)   faca

      se (cont%2 = 0) entao
         escreva(cont," - ")
      fimse
      cont <- cont + 1
   fimenquanto

Fimalgoritmo